#define string_size 		60					// –º–∞–∫—Å_–∫–æ–ª–∏—á–µ—Å—Ç–≤–æ_—Å–∏–º–≤–æ–ª–æ–≤_–≤_—Å—Ç—Ä–æ–∫–µ
#define string_amount 		163					// –º–∞–∫—Å_–∫–æ–ª–∏—á–µ—Å—Ç–≤–æ_—Å—Ç—Ä–æ–∫
#define string_pack_amount 	36					// –º–∞–∫—Å_–∫–æ–ª–∏—á–µ—Å—Ç–≤–æ_—Å—Ç—Ä–æ–∫_–≤_1-–æ–º_–ø–∞–∫–µ—Ç–µ

#define start_serv_string	155					// –Ω–∞—á–∞–ª–æ_—Å–ª—É–∂–µ–±–Ω—ã—Ö_—Å—Ç—Ä–æ–∫
#define start_ctrl_string	150					// –Ω–∞—á–∞–ª–æ_—Å—Ç—Ä–æ–∫_–∫–æ–Ω—Ç—Ä–æ–ª—è

#define degree_sym_amount	4					// –º–∞–∫—Å–∏–º–∞–ª—å–Ω–æ–µ_–∫–æ–ª–∏—á–µ—Å—Ç–≤–æ_–∑–Ω–∞–∫–æ–≤_–≥—Ä–∞–¥—É—Å–∞_–≤_—Å—Ç—Ä–æ–∫–µ
#define UartBufSize 		4096				// —Ä–∞–∑–º–µ—Ä_–±—É—Ñ–µ—Ä–∞_UART

#define UP_TIME 			86400*2				// –≤—Ä–µ–º—è_–∑–∞—Å—ã–ø–∞–Ω–∏—è_–¥–∏—Å–ø–ª–µ—è
#define UP_TIME_small		240					// –≤—Ä–µ–º—è_–∑–∞—Å—ã–ø–∞–Ω–∏—è_–¥–∏—Å–ø–ª–µ—è_–≤_—Ä–µ–∂–º–µ_—Å–Ω–∞
#define transmit_time		500					// –ø–µ—Ä–∏–æ–¥_–æ—Ç–ø—Ä–∞–≤–æ–∫_–Ω–∞_–º–∞—Ç–µ—Ä–∏–Ω–∫—É

#define scroll_speed		15					// —Å–∫–æ—Ä–æ—Å—Ç—å_–ø—Ä–æ–ª–∏—Å—Ç—ã–≤–∞–Ω–∏—è_—Å—Ç—Ä–æ–∫_–ø—Ä–∏_–¥–æ–ª–≥–æ–º_–Ω–∞–∂–∞—Ç–∏–∏ (—Å—Ç—Ä–æ–∫_–≤_—Å–µ–∫—É–Ω–¥—É)
#define scroll_delay		600					// –≤—Ä–µ–º—è_—É–¥–µ—Ä–∂–∞–Ω–∏—è_–∫–Ω–æ–ø–∫–∏_–¥–æ_–Ω–∞—á–∞–ª–∞_–ø—Ä–æ–ª–∏—Å—Ç—ã–≤–∞–Ω–∏—è (–º—Å)
#define OFF_delay			3000				// –≤—Ä–µ–º—è_—É–¥–µ—Ä–∂–∞–Ω–∏—è_–∫–Ω–æ–ø–∫–∏_–≤—ã–∫–ª—é—á–µ–Ω–∏—è_–¥–æ_–∑–∞–ø—É—Å–∫–∞_–æ—Ç–∫–ª—é—á–µ–Ω–∏—è (–º—Å)
#define PowerOFF_delay		5000				// –∑–∞–¥–µ—Ä–∂–∫–∞_–ø–æ—Å–ª–µ_–Ω–∞—á–∞–ª–∞_–≤—ã–∫–ª—é—á–µ–Ω–∏—è
#define start_pause			600					// –∑–∞–¥–µ—Ä–∂–∫–∞_–ø–µ—Ä–µ–¥_—Å—Ç–∞—Ä—Ç–æ–º_–¥–∏—Å–ø–ª–µ—è (–¥–ª–∏—Ç–µ–ª—å–Ω–æ—Å—Ç—å_—Å—Ç–∞—Ä—Ç–æ–≤–æ–≥–æ_–º–∏–≥–∞–Ω–∏—è)
#define Button_Zummer		10					// –∑–∞–¥–µ—Ä–∂–∫–∞_–Ω–∞_–ø–∏—Å–∫_–∑—É–º–µ—Ä–∞_–ø—Ä–∏_–Ω–∞–∂–∞—Ç–∏–∏_–∫–Ω–æ–ø–∫–∏


#define pack_DELAY			150					// –∑–∞–¥–µ—Ä–∂–∫–∞_–º–µ–∂–¥—É_–ø–æ–ª—É—á–µ–Ω–∏–µ–º_–ø–∞–∫–µ—Ç–∞_–∏_–Ω–∞—á–∞–ª–æ–º_–ø–∞—Ä—Å–∏–Ω–≥–∞
#define restart_time		550					// –≤—Ä–µ–º—è_–ø–æ—Å–ª–µ_–∫–æ—Ç–æ—Ä–æ–≥–æ_–ø–µ—Ä–µ–∑–∞–ø—É—Å–∫–∞–µ—Ç—Å—è_UART_–ø—Ä–∏_–æ—Ç—Å—É—Ç—Å—Ç–≤–∏–∏_–ø—Ä–∏–µ–º–∞_–¥–∞–Ω–Ω—ã—Ö

/*-------------------------- –†–ê–°–ßÔøΩ?–¢–ù–´–ï –ó–ù–ê–ß–ï–ùÔøΩ?–Ø ------------------------------*/
#define serv_string_amnt	(string_amount - start_serv_string)		// –ö–æ–ª–∏—á–µ—Å—Ç–≤–æ_—Å–ª—É–∂–µ–±–Ω—ã—Ö_—Å—Ç—Ä–æ–∫
#define ctrl_string_amnt	(start_serv_string - start_ctrl_string)	// –ö–æ–ª–∏—á–µ—Å—Ç–≤–æ_—Å—Ç—Ä–æ–∫_–∫–æ–Ω—Ç—Ä–æ–ª—è

			/*- - - - - - - - - –ù–æ–º–µ—Ä–∞_–°—Ç—Ä–æ–∫ - - - - - - - - - */
#define serv_string_1		start_serv_string						// 1-—è_—Å–ª—É–∂–µ–±–Ω–∞—è_—Å—Ç—Ä–æ–∫–∞
#define serv_string_2		start_serv_string + 1					// 2-—è_—Å–ª—É–∂–µ–±–Ω–∞—è_—Å—Ç—Ä–æ–∫–∞

#define ctrl_string_1		start_ctrl_string						// 1-—è_—Å—Ç—Ä–æ–∫–∞_–∫–æ–Ω—Ç—Ä–æ–ª—è
#define ctrl_string_2		start_ctrl_string + 1					// 2-—è_—Å—Ç—Ä–æ–∫–∞_–∫–æ–Ω—Ç—Ä–æ–ª—è
#define ctrl_string_3		start_ctrl_string + 2					// 3-—è_—Å—Ç—Ä–æ–∫–∞_–∫–æ–Ω—Ç—Ä–æ–ª—è
#define ctrl_string_4		start_ctrl_string + 3					// 4-—è_—Å—Ç—Ä–æ–∫–∞_–∫–æ–Ω—Ç—Ä–æ–ª—è
#define ctrl_string_5		start_ctrl_string + 4					// 5-—è_—Å—Ç—Ä–æ–∫–∞_–∫–æ–Ω—Ç—Ä–æ–ª—è

/*----------------------- –î–ê–¢–ßÔøΩ?–ö –¢–ï–ú–ü–ï–†–ê–¢–£–†–´ STM32 ---------------------------*/
#define TEMP110_CAL_ADDR 	((uint16_t*) ((uint32_t) 0x1FFF7A2E))	// –∞–¥—Ä–µ—Å_–∫–∞–ª–∏–±—Ä–æ–≤–∞–Ω–æ–≥–æ_–∑–Ω–∞—á–µ–Ω–∏—è_–ø—Ä–∏_T = 110
