/*------------------------------- STRUCTUREs ---------------------------------*/

/*
 * –°–¢–†–£–ö–¢–£–†–ê –°–¢–†–û–ö.
 * –í –ù–ï–ï –ö–õ–ê–î–£–¢–°–Ø –ü–†ÔøΩ?–ù–Ø–¢–´–ï ÔøΩ? –û–¢–ü–†–ê–í–õ–Ø–ï–ú–´–ï –°–¢–†–û–öÔøΩ?.
 */
typedef struct{
	uint8_t buf[string_size];						// –±—É—Ñ–µ—Ä_—Å—Ç—Ä–æ–∫–∏
	uint8_t number;									// –Ω–æ–º–µ—Ä_—Å—Ç—Ä–æ–∫–∏
	uint8_t status;									// —Å—Ç–∞—Ç—É—Å_—Å—Ç—Ä–æ–∫–∏ (–º–æ–∂–Ω–æ_–ª–∏_–µ–µ_–≤—ã–≤–æ–¥–∏—Ç—å)
	uint8_t degreeSym[degree_sym_amount];			// –º–∞—Å—Å–∏–≤_—Ö—Ä–∞–Ω—è—â–∏–π_–ø–æ–ª–æ–∂–µ–Ω–∏–µ_–≤_—Å—Ç—Ä–æ–∫–µ_–∑–Ω–∞–∫–æ–≤_–≥—Ä–∞–¥—É—Å–∞ (–¥–æ degree_sym_amount)
}string_t;

/*
 * –°–¢–†–£–ö–¢–£–†–ê –î–õ–Ø –ë–£–§–ï–†–û–í.
 * –ü–õ–ê–ùÔøΩ?–†–û–í–ê–õ–ê–°–¨ –£–ùÔøΩ?–í–ï–†–°–ê–õ–¨–ù–û–ô, –ù–û ÔøΩ?–°–ü–û–õ–¨–ó–£–ï–¢–°–Ø
 * –¢–û–õ–¨–ö–û –î–õ–Ø –†–ê–°–ß–ï–¢–ê CRC32
 */
typedef struct{
	uint8_t buf[string_pack_amount*string_size];	// –æ—Å–Ω–æ–≤–Ω–æ–π_–±—É—Ñ–µ—Ä
	uint16_t tail;									// —Ö–≤–æ—Å—Ç_–±—É—Ñ–µ—Ä–∞ (–±—É—Ñ–µ—Ä_–Ω–µ_—Ü–∏–∫–ª–∏—á–Ω—ã–π, –ø–æ—ç—Ç–æ–º—É_–∏_–∫–æ–ª–∏—á–µ—Å—Ç–≤–æ_—Å–∏–º–≤–æ–ª–æ–≤_–≤_–±—É—Ñ–µ—Ä–µ)
	uint16_t head;									// –≥–æ–ª–æ–≤–∞_–±—É—Ñ–µ—Ä–∞ (–Ω–∞—á–∞–ª–æ_,_–æ—Å—Ç–∞–≤–ª–µ–Ω_–ø—Ä–∏_–Ω–µ–æ–±—Ö–æ–¥–∏–º–æ—Å—Ç–∏_—Å–¥–µ–ª–∞—Ç—å_—Ü–∏–∫–ª–∏—á–Ω—ã–π_–±—É—Ñ–µ—Ä)
}buffer_t;

typedef struct{
	uint8_t LED_status;
	GPIO_TypeDef *LED_port;
	uint16_t LED_pin;
	uint32_t timer;
}led_status;
/*
 * –°–¢–†–£–ö–¢–£–†–ê –°–û–°–¢–û–Ø–ùÔøΩ?–ô –°–í–ï–¢–û–îÔøΩ?–û–î–û–í RGB
 * –•–†–ê–ùÔøΩ?–¢ –†–ï–ñÔøΩ?–ú–´ –†–ê–ë–û–¢–´ –ö–ê–ñ–î–û–ì–û ÔøΩ?–ó 3-—Ö –°–í–ï–¢–û–îÔøΩ?–û–î–û–í
 */
typedef struct{
	led_status RED;									// –∫—Ä–∞—Å–Ω—ã–π_—Å–≤–µ—Ç–æ–¥–∏–æ–¥
	led_status GREEN;								// –∑–µ–ª–µ–Ω—ã–π_-//-
	led_status BLUE;								// —Å–∏–Ω–∏–π_  -//-
}RGB_status;

/*
 * –°–¢–†–£–ö–¢–£–†–ê –°–û–°–¢–û–Ø–ùÔøΩ?–Ø –£–ü–†–ê–í–õ–Ø–ï–ú–´–• –°ÔøΩ?–°–¢–ï–ú
 * –•–†–ê–ùÔøΩ?–¢ –°–û–°–¢–û–Ø–ùÔøΩ?–ï –†–ï–õ–ï, DC ÔøΩ? –îÔøΩ?–°–ü–õ–ï–Ø
 */
typedef struct{
	uint8_t RELE[4];
	uint8_t DC;
	uint8_t DISP;
}ctrl_status;

